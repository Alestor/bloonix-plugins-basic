chart {
    id 1
    title SNMP if - errors, discards
    options {
        ylabel packets in(+) / out(-)
        units default
        chart-type area
        series {
            name in_errors
            color \#9a72ad
        }
        series {
            name in_discards
            color \#e9644a
        }
        series {
            name out_errors
            color \#ffb244
            opposite true
        }
        series {
            name out_discards
            color \#2ba743
            opposite true
        }
    }
}

chart {
    id 2
    title SNMP if - in/out octets
    options {
        ylabel octets
        units bytes_to_bits
        chart-type area
        series {
            name in_octets
            color \#005467
        }
        series {
            name out_octets
            color \#ff7a0d
            opposite true
        }
    }
}

chart {
    id 3
    title SNMP if - multicast, broadcast
    options {
        ylabel packets
        units default
        chart-type area
        series {
            name in_ucast_pkts
            color \#ffb244
        }
        series {
            name in_nucast_pkts
            color \#9a72ad
        }
        series {
            name in_multicast_pkts
            color \#005467
        }
        series {
            name in_broadcast_pkts
            color \#e9644a
        }
        series {
            name out_ucast_pkts
            color \#ffb244
        }
        series {
            name out_nucast_pkts
            color \#9a72ad
        }
        series {
            name out_multicast_pkts
            color \#005467
        }
        series {
            name out_broadcast_pkts
            color \#e9644a
        }
    }
}
