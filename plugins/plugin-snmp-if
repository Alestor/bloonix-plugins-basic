plugin {
    id 12
    plugin SNMP.Interface
    command check-snmp-if
    datatype statistic
    category Network,SNMP
    netaccess yes
    prefer localhost
    abstract Check snmp-if
    description Check the interfaces of routers and switches.
}

statistic {
    statkey admin_status
    alias ifAdminStatus
    datatype varchar(20)
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.7
}

statistic {
    statkey oper_status
    alias ifOperStatus
    datatype varchar(20)
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.8
}

statistic {
    statkey in_discards
    alias ifInDiscards
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.13
}

statistic {
    statkey in_errors
    alias ifInErrors
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.14
}

statistic {
    statkey out_discards
    alias ifOutDiscards
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.19
}

statistic {
    statkey out_errors
    alias ifOutErrors
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.20
}

statistic {
    statkey in_octets
    alias ifInOctets
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.10, 1.3.6.1.2.1.31.1.1.1.6
}

statistic {
    statkey in_ucast_pkts
    alias ifInUcastPkts
    datatype float
    units bytes
    description OID 1.3.6.1.2.1.2.2.1.11, 1.3.6.1.2.1.31.1.1.1.7
}

